{
  "name": "@totalpave/bowser",
  "version": "1.0.0-beta.1",
  "description": "Lightweight browser detector",
  "keywords": [
    "browser",
    "useragent",
    "user-agent",
    "parser",
    "ua",
    "detection",
    "ender",
    "sniff"
  ],
  "homepage": "https://github.com/lancedikson/bowser",
  "author": "Dustin Diaz <dustin@dustindiaz.com> (http://dustindiaz.com)",
  "contributors": [
    {
      "name": "Denis Demchenko",
      "url": "http://twitter.com/lancedikson"
    }
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lancedikson/bowser.git"
  },
  "devDependencies": {
    "@totalpave/eslint-plugin": "2.3.2",
    "@types/jasmine": "3.5.0",
    "@types/node": "12.12.21",
    "@types/yamljs": "0.2.30",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "docdash": "1.1.1",
    "eslint": "6.8.0",
    "jasmine": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "jsdoc": "3.6.3",
    "nyc": "14.1.1",
    "typescript": "3.7.4",
    "yamljs": "0.3.0"
  },
  "bugs": {
    "url": "https://github.com/lancedikson/bowser/issues"
  },
  "scripts": {
    "build": "npm run-script lint && tsc",
    "generate-and-deploy-docs": "npm run generate-docs && gh-pages --dist docs --dest docs",
    "watch": "webpack --watch --config webpack.config.js",
    "deploy": "jsdoc -c jsdoc.json && gh-pages --dist docs --dest docs",
    "prepublishOnly": "npm run build",
    "lint": "node --max_old_space_size=6144 ./node_modules/eslint/bin/eslint.js --ext .ts '?(src|spec)/**/*.ts' --cache",
    "testem": "testem",
    "test": "npm run-script lint && nyc --reporter=lcov --reporter=text ts-node ./spec/support/jasmine.ts",
    "test:watch": "ava --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "generate-docs": "jsdoc -c jsdoc.json"
  },
  "license": "MIT",
  "dependencies": {
    "tslib": "1.10.0"
  }
}
